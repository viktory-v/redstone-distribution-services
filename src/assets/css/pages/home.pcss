@import '../global-mixins.pcss';

.section-servicing {
  margin-top: 120px;
  background-color: #ecf7f9;

  .inner {
    display: flex;
    padding-top: 82px;
  }

  .title {
    font-family: Krub;
    font-style: normal;
    font-weight: normal;
    font-size: 40px;
    line-height: 54px;
    color: #404554;
    max-width: 788px;
    width: 100%;
    padding-bottom: 90px;
    padding-top: 46px;
    padding-right: 126px;
    border-bottom: 4px solid #fff;
    margin-bottom: 0;

    b {
      color: #000;
    }
  }

  .text {
    margin-bottom: 102px;
    margin-top: 58px;

    span {
      font-style: normal;
      font-weight: 500;
      font-size: 14px;
      line-height: 20px;
      text-transform: uppercase;
      color: #27559a;
      width: 100%;
      display: block;
      margin-bottom: 3px;
    }

    p {
      font-style: normal;
      font-weight: normal;
      font-size: 30px;
      line-height: 43px;
      color: #001730;
      display: inline-block;
      margin-right: 25px;
    }

    a {
      display: inline-block;
      text-decoration: none;
      font-weight: 500;
      font-size: 16px;
      line-height: 23px;
      color: #e91716;
      position: relative;

      &::after {
        content: '';
        position: absolute;
        background-image: url(../../images/servicing-arrow.svg);
        width: 15px;
        height: 12px;
        top: 7px;
        left: 94px;
      }
    }
  }

  .img {
    width: 423px;
    height: 378px;
  }

  .slider {
    @extend .slick-not-initialized;

    padding-top: 44px;

    .slick-list {
      width: 100%;
      height: 374px;
    }

    .slick-dots {
      position: absolute;
      width: 100%;
      padding: 0;
      margin: 0;
      list-style: none;
      text-align: center;
      display: flex;
      justify-content: flex-end;
      bottom: -42px;
      right: 10px;

      li {
        margin: 0;
        position: relative;
        display: inline-block;
        width: 18px;
        height: 20px;
        padding: 0;
        cursor: pointer;
      }

      & li.slick-active button {
        background: #0ca8e7;
      }

      & li button {
        font-size: 0;
        line-height: 0;
        display: block;
        padding: 5px;
        cursor: pointer;
        color: transparent;
        border: 0;
        outline: none;
        width: 10px;
        height: 9px;
        margin: 0 auto;
        border-radius: 50%;
        background: #fff;
      }

      & li button::before {
        display: none;
      }
    }
  }

  .col {
    &:last-child {
      border: 5px solid #fff;
      height: 476px;
      max-width: 433px;
      width: 100%;
    }
  }

  @media (max-width: 1240px) {
    .inner {
      padding: 42px 28px 2px;
      justify-content: space-between;
    }

    .title {
      padding: 22px 51px 57px 2px;
      max-width: 537px;
      width: 100%;
    }

    .text {
      margin-left: 2px;
      margin-bottom: 70px;

      span {
        margin-bottom: 5px;
      }

      a {
        display: block;
        margin-top: 19px;
      }
    }
  }

  @media (max-width: 990px) {
    .inner {
      flex-direction: column-reverse;
      align-items: center;
      padding: 0 50px 2px;
    }

    .title {
      max-width: none;
      padding-right: 0;
      padding-bottom: 39px;
      padding-left: 0;
    }

    .text {
      margin-top: 38px;
      margin-bottom: 68px;
      margin-left: 0;

      a {
        display: inline-block;
      }

      span {
        margin-bottom: 2px;
      }
    }

    .col {
      &:last-child {
        max-width: none;
        margin-top: 40px;
        height: 350px;
      }

      &:first-child {
        padding-top: 28px;
      }
    }

    .slider {
      padding-top: 0;

      .slick-dots {
        bottom: 40px;
      }
    }

    .slick-slide {
      img {
        height: 338px;
        width: 100%;
        object-fit: contain;
        padding-right: 20px;
      }
    }
  }

  @media (max-width: 750px) {
    margin-top: 111px;

    .inner {
      padding-left: 25px;
      padding-right: 20px;
      padding-bottom: 17px;
      flex-direction: row;
      align-items: flex-start;
    }

    .title {
      font-size: 28px;
      line-height: 40px;
      padding-right: 17px;
      padding-top: 0;
      padding-bottom: 28px;
    }

    .text {
      margin-top: 29px;
      margin-left: 0;
      margin-right: -300px;

      p {
        font-size: 24px;
        line-height: 35px;
        margin-right: 16px;
      }

      span {
        margin-bottom: -6px;
      }

      a {
        margin-top: 17px;
      }
    }

    .col {
      &:last-child {
        max-width: 329px;
        margin-top: 15px;
        height: 362px;
      }

      &:first-child {
        padding-top: 25px;
      }
    }

    .slick-slide {
      img {
        padding-right: 0;
        padding-top: 13px;
      }
    }

    .slider {
      .slick-dots {
        bottom: 21px;

        li {
          width: 13px;

          button {
            padding: 4px;
            width: 6px;
            height: 3px;
          }
        }
      }
    }
  }

  @media (max-width: 630px) {
    .inner {
      padding: 0 15px 12px;
      flex-direction: column-reverse;
    }

    .text {
      margin-right: 0;
    }

    .title {
      padding-top: 5px;
      padding-left: 5px;
      padding-bottom: 19px;
    }

    .text {
      padding-left: 5px;

      p {
        margin-right: 0;
        margin-top: 9px;
      }

      a {
        margin-top: 29px;
      }
    }

    .slick-slide {
      img {
        height: 287px;
        margin-top: 32px;
        margin-left: 0;
        padding-top: 0;
      }
    }
  }
}

.section-description {
  position: relative;

  .inner {
    display: flex;
    padding-bottom: 100px;
  }

  .text {
    padding-top: 119px;
    font-style: normal;
    font-weight: 300;
    font-size: 22px;
    line-height: 35px;
    color: #fff;
    max-width: 926px;

    b {
      letter-spacing: 0.6px;
    }
  }

  .products-lists {
    display: flex;
    margin-top: 91px;

    .list {
      margin-right: 87px;
      margin-left: 3px;
      max-width: 224px;

      &:nth-child(3) {
        margin-left: 26px;
