      margin-top: 72px;

      .title {
        padding-bottom: 24px;

        &::before {
          bottom: 3px;
          left: -2px;
        }
      }
    }

    .col {
      &:last-child {
        margin-top: 36px;
        margin-left: 2px;
      }
    }
  }

  @media (max-width: 990px) {
    .inner {
      padding: 0 50px 37px;
    }

    .text {
      padding-left: 5px;
    }

    .products-lists {
      flex-wrap: wrap;
      margin-top: 70px;

      .list {
        margin-left: 0;
        margin-right: 110px;

        &:nth-child(2) {
          padding-top: 2px;
        }

        &:nth-child(3) {
          margin-left: 0;
          margin-top: -92px;
        }
      }

      .title {
        padding-bottom: 21px;
        margin-bottom: 43px;

        &::before {
          bottom: 0;
          left: 0;
        }
      }
    }

    .col {
      &:last-child {
        margin-top: 21px;
        margin-left: 0;
      }
    }

    .btn {
      padding: 2px 49px 6px 27px;
    }
  }

  @media (max-width: 750px) {
    .inner {
      padding-left: 15px;
      padding-right: 20px;
      padding-bottom: 36px;
    }

    .text {
      padding-top: 46px;
    }

    .products-lists {
      margin-top: 39px;
      margin-left: 5px;

      .list {
        margin-right: 76px;

        &:nth-child(3) {
          margin-top: -111px;
        }
      }
    }

    .col {
      &:last-child {
        justify-content: center;
        margin-top: 41px;
      }
    }

    .btn {
      padding: 2px 50px 6px 27px;
      margin-left: 6px;
      margin-bottom: 4px;
    }
  }

  @media (max-width: 630px) {
    .inner {
      padding-bottom: 46px;
    }

    .text {
      padding-top: 39px;
      padding-right: 5px;
    }

    .products-lists {
      .list:nth-child(2) {
        padding-top: 17px;
      }

      .list:nth-child(3) {
        margin-top: 36px;
      }
    }
  }
}

.section-products-procedure {
