    .produres-list {
      justify-content: center;
      align-items: center;

      .item a {
        flex-direction: column;
        margin-bottom: 26px;
      }

      .img {
        height: 177px;
      }

      .content {
        padding: 32px 19px 10px;
      }
    }
  }
}

.section-form {
  position: relative;

  .inner {
    display: flex;
    padding-bottom: 100px;
  }

  .title {
    font-family: Krub;
    font-style: normal;
    font-weight: bold;
    font-size: 40px;
    line-height: 52px;
    color: #fff;
    margin-bottom: 22px;
    max-width: 405px;
  }

  .text {
    font-family: Noto Sans TC;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 23px;
    color: #fff;
  }

  .col {
    margin-top: 98px;

    &:first-child {
      max-width: 487px;
      width: 100%;
    }

    &:last-child {
      width: 100%;
    }
  }

  .gform_wrapper {
    margin-top: 18px;

    .gform_button {
      margin-top: 40px;
    }
  }

  .gfield_checkbox {
    display: flex;
    font-family: Noto Sans TC;
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 24px;
    color: #fff;
    max-width: 709px;
    width: 100%;
    justify-content: space-between;
    margin-bottom: 21px;

    input[type='checkbox'] {
      position: absolute;
      opacity: 0;
      cursor: pointer;
      height: 0;
      width: 0;

      &:checked + label::before {
        background-color: #fff;
        border-color: #fff;
        background-image: url(../../images/checkmark.svg);
        background-repeat: no-repeat;
        background-position: center;
      }
    }

    label {
      position: relative;
      padding-left: 54px;

      &::before {
        content: '';
        position: absolute;
        width: 34px;
        height: 34px;
        border: 1px solid rgba(255, 255, 255, 0.353557);
        border-radius: 3px;
        left: 0;
        top: -4px;
      }
    }
  }

  .gf_left_half {
    width: calc(50% - 20px);
  }

  .gf_right_half {
    width: calc(50% - 20px);
    margin-left: 36px;
  }

  .gfield_radio {
    display: flex;
    margin: 30px 0 -75px;
    pointer-events: none;

    & > * {
      flex: 1;
      margin-right: 12px;
      margin-bottom: 10px;

      &:first-child {
        margin-right: 51px;
      }
    }

    input[type='radio'] {
      display: none;

      & + label {
        border: 1px solid rgba(164, 171, 193, 0.501311);
        box-sizing: border-box;
        border-radius: 3px;
        width: 35px;
        height: 35px;
        display: block;
        margin: 0 0 0 auto;
        overflow: hidden;
        text-indent: 1000px;
        pointer-events: all;
      }

      &:checked + label {
        background-color: #fff;
        border-color: #fff;
        background-image: url(../../images/checkmark.svg);
        background-repeat: no-repeat;
        background-position: center;
      }
    }
  }

  .gfield_label {
    font-family: Noto Sans TC;
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 24px;
    color: #fff;
    margin-top: 34px;
    display: inline-block;

    span {
      display: none;
    }
  }

  .gform_footer {
    text-align: right;
    float: right;
    width: 100%;
  }

  @media (max-width: 1240px) {
    .inner {
      flex-direction: column;
      padding: 0 178px 59px;
    }

    .title {
      max-width: 100%;
      padding-left: 10px;
    }

    .text {
      padding-left: 10px;
    }

    .col {
      margin-top: 61px;

      &:first-child {
        max-width: 668px;
        width: 100%;
      }
    }

    .gfield_checkbox {
      padding: 0 12px 0 0;

      li:nth-child(2) {
        margin-left: -8px;
      }
    }

    .gf_left_half {
      width: calc(50% - 7px);
    }

    .gf_right_half {
      width: calc(50% - 7px);
      margin-left: 10px;
    }

    .gfield_radio {
      & > *:first-child {
        margin-right: 24px;
      }
    }
  }

  @media (max-width: 990px) {
    .inner {
      padding: 0 50px 61px;
    }

    .col {
      margin-top: 60px;
    }

    .ginput_container_text {
      padding-bottom: 4px;
      padding-top: 2px;
      margin-top: 22px;
    }

    .gform_wrapper {
      .gform_button {
        margin-top: 38px;
      }
    }

    .gfield_label {
      margin-top: 29px;
    }
  }

  @media (max-width: 750px) {
    .inner {
      padding-left: 20px;
      padding-right: 20px;
      padding-bottom: 60px;
    }

    .gfield_checkbox {
      flex-wrap: wrap;
      padding-right: 0;
      justify-content: flex-start;
      margin-bottom: 0;

      li {
        &,
        &:nth-child(2) {
          margin-left: 0;
          margin-right: 30px;
          display: flex;
          height: 36px;
        }

        &:nth-child(3) {
          margin-top: 20px;
        }
      }
    }

    .ginput_container_text {
      margin-top: 4px;
    }

    .gform_wrapper {
      .gform_button {
        margin-top: 39px;
      }
    }
  }

  @media (max-width: 630px) {
    .inner {
      padding: 0 20px 60px;
    }

    .title {
      font-size: 24px;
      line-height: 31px;
      padding-left: 0;
      padding-right: 5px;
    }

    .text {
      padding-left: 0;
    }

    .col {
      &:last-child {
        margin-top: 17px;
      }
    }

    .gfield_checkbox {
      margin-bottom: 1px;

      li {
        margin-bottom: 20px;

        &:nth-child(3) {
          margin-top: 0;
        }
      }
    }

    .gform_wrapper .gf_right_half,
    .gform_wrapper .gf_left_half {
      float: none;
      width: 100%;
      margin-left: 0;
    }

    #field_13_80 {
      position: relative;
    }

    .ginput_container_text {
      margin-bottom: 15px;
    }

    #input_11_2,
    #input_15_2,
    #input_2_2,
    #input_19_2,
    #input_35_2 {
      margin-top: 0;
    }

    .gfield_label {
      margin-top: 15px;
    }

    .gform_wrapper .gform_button {
      margin-top: 30px;
      width: 100%;
    }

    .gfield_radio {
      flex-direction: column;
      position: absolute;
      right: 0;

      & > *:first-child {
        margin-right: 12px;
      }

      & > *:last-child {
        margin-top: 31px;
      }
    }
  }

  @media (max-width: 400px) {
    .gform_button {
      background-position: 68% center;
    }
  }
}

